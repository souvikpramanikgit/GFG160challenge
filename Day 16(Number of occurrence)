class Solution {
  public:
    int countFreq(vector<int>& arr, int target) {
        // code here
        
        int count=0;
        int n=arr.size();
        int l=0,r=n-1;
        while(l<r){
            int mid=(l+r)/2;
            if(arr[mid]==target){
                if(arr[mid-1]==target)
                count++;
                
            }
        }
        return count;
    }
};

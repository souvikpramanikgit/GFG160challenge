// Tree Node
class Node {
  public:
    int data;
    Node* left;
    Node* right;

    // Constructor to initialize a new node
    Node(int val) {
        data = val;
        left = NULL;
        right = NULL;
    }
};
*/

class Solution {
  public:
    // Function to return a list containing the inorder traversal of the tree.
    void ans(Node* root,vector<int> &arr){
        if(!root) return;
        
        ans(root->left,arr);
        arr.push_back(root->data);
        ans(root->right,arr);
    }
    vector<int> inOrder(Node* root) {
        // Your code here
        vector<int> arr;
        ans(root,arr);
        return arr;
    }
};
